    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vitest": "^0.34.0"
  },
  "dependencies": {
    "@urbaninstitute/dataviz-components": "^0.0.2",
    "d3": "^7.8.5",
    "mapbox-gl": "^3.3.0",
    "pym.js": "^1.3.2",
    "svelte-select": "^5.8.1",
    "svelte-typeahead": "^4.4.1",
    "vite-plugin-svelte-svg": "^2.3.0",
    "yootils": "^0.3.1"
